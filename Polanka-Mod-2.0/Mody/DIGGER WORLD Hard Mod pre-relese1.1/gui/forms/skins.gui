					profile = "SkinWindowNormalCaptionText";
					text = %header;
				};
			};
			
			// Skin description
			new GuiMLTextCtrl()
			{
				position = "0 0";
				canHit = false; 
				extent = "445 10";
				profile = "SkinWindowNotificationText";
				text = %desc;
			};
			
			new GuiMLTextCtrl()
			{
				visible = !%canBuy;
				canHit = false; 
				profile = "SkinWindowNormalDescriptionText";
				text = GetMessageIDText(4113); // Lootbox notification
			};
			
			// Skin footer
			new GuiFlexCtrl()
			{
				profile = "SkinsWindowFlex";
				position = "0 0";
				extent = "445 74";
				justify = "stretch";
				align = "center";
				visible = %canBuy;
				marginBottom = 30;
				
				new guiHorzTilingButtonCtrl()
				{
					text = GetMessageIDText(4115); // Go to shop
					internalName = "buyButton";
					horizSizing = "relative";
					position = "0 0";
					maxExtent = "158 44";
					extent = "158 44";
					minExtent = "8 2";
					profile = "SkinsWindowButton";
					useMouseEvents = "1";
					visible = %canBuy;
					active = %canBuy;
					command = "";
					imageIndex = GoldBtn;
				};
			};
		};
	};
	
	return %guiContent;
}

