// Skin Select

function createSkinsSelectElement(%icon, %header, %desc, %status)
{	
	%canBuy = getWord(%status, 0);
	%price = getWord(%status, 1);
	%equipStatus = getWord(%status, 2);
	
	%onlyWin = (%canBuy == false) && (%equipStatus < 2);
	%canBuy = (%canBuy == true) && (%equipStatus < 2);
	%isPriceVisible = (%price > 0);
	
	%guiContent = new GuiFlexCtrl()
	{
		position = "0 0";
		extent = "585 10";
		horizSizing = "width";
		align = "top";
		justify = "stretch";
		dynamicHeight = true;
		
		new GuiAbsoluteFlexCtrl()
		{
			// Clothed background
			new GuiBitmapCtrl()
			{
				internalName = "clothedBackground";
				extent = "100% 100%";
				horizSizing = "width";
				vertSizing = "height";
				profile = "GuiAtlas3ImageProfile";
				imageIndex = LearningWindowBackgroundTransparent;
				canHit = false; 
				resize = false;
				wrap = true;
			};
			
			// Mouse events
			new GuiMouseEventCtrl()
			{
				internalName = "mouseControl";
				extent = "100% 100%";
				horizSizing = "width";
				vertSizing = "height";
				class = "SelectSkinsMouseControl";
			};
		};
		
		// Skin Icon
		new GuiFlexCtrl()
		{
			position = "0 0";
			extent = "130 220";
			direction = "column";
			dynamicHeight = true;
			canHit = false; 
			align = "center";
			marginTop = 32;
			
			new GuiBitmapCtrl()
			{
				profile = "GuiDefaultProfile";
				bitmap = %icon;
				centered = false;
				canHit = false; 
				wrap = false;
				resize = true;
			};
			
			new GuiTextCtrl()
			{
				internalName = "clothedText";
				profile = "SkinWindowClothedText";
				text = GetMessageIDText(4111); // Clothed
				visible = false;
			};
		};
		
		// Skin content
		new GuiFlexCtrl()
		{
			profile = "SkinsWindowFlex";
			position = "0 0";
			extent = "445 100";
			horizSizing = "relative";
			direction = "column";
			dynamicHeight = true;
			
			// Skin header
			new GuiFlexCtrl()
			{
				position = "0 0";
				extent = "445 60";
				justify = "stretch";
				align = "start";
				canHit = false; 
				dynamicHeight = true;
				marginTop = 23;
				
				new GuiMLTextCtrl()
				{
					horizSizing = "relative";
					profile = "SkinWindowNormalCaptionText";
					text = %header;
				};
				
				new GuiBitmapCtrl()
				{
					extent = "15 16";
					profile = "GuiAtlas3ImageProfile";
					imageIndex = LightTimeIcon;
					centered = false;
					canHit = false; 
					wrap = false;
					resize = false;
					visible = (%equipStatus == 1);
				};
				
				new GuiMLTextCtrl()
				{
					internalName = "premiumTimer";
					profile = "SkinWindowTimerText";
					text = "available\n15:05:23";
					visible = (%equipStatus == 1);
				};
			};
			
			// Skin description
			new GuiMLTextCtrl()
			{
				position = "0 0";
				canHit = false; 
				extent = "445 10";
				profile = "SkinWindowNormalDescriptionText";
				text = %desc;
			};
			
			new GuiMLTextCtrl()
			{
				visible = (%equipStatus == 1);
				canHit = false; 
				internalName = "subscriptionText";
				profile = "SkinWindowNotificationText";
				text = GetMessageIDText(4112); // Gift notification
			};
			
			new GuiMLTextCtrl()
			{
				visible = %onlyWin;
				canHit = false; 
				internalName = "lootboxText";
				profile = "SkinWindowNotificationText";
				text = GetMessageIDText(4113); // Can't buy notication
			};
			
			// Skin footer
			new GuiFlexCtrl()
			{
				profile = "SkinsWindowFlex";
				position = "0 0";
				extent = "445 74";
				justify = "stretch";
				align = "center";
				marginBottom = 30;
				padding = "3 0";
				
				// Buttons
				new GuiFlexCtrl()
				{
					profile = "SkinsWindowFlex";
					position = "0 6";
					extent = "100 44";
					padding = "10 0";
					horizSizing = "relative";
					justify = "stretch";
					
					new guiHorzTilingButtonCtrl()
					{
						text = GetMessageIDText(4114); // Equip now
						internalName = "equipButton";
						horizSizing = "relative";
						position = "0 0";
						maxExtent = "158 44";
						extent = "158 44";
						minExtent = "8 2";
						profile = "SkinsWindowButton";
						useMouseEvents = "1";
						visible = (%equipStatus > 0);
						active = (%equipStatus > 0);
						command = "";
						imageIndex = H44Btn;
					};
					
					new guiHorzTilingButtonCtrl()
					{
						text = GetMessageIDText(4115); // Go to shop
						internalName = "buyButton";
						horizSizing = "relative";
						position = "0 0";
						maxExtent = "158 44";
						extent = "158 44";
						minExtent = "8 2";
						profile = "SkinsWindowButton";
						useMouseEvents = "1";
						visible = %canBuy;
						active = %canBuy;
						command = "";
						imageIndex = GoldBtn;
					};
				};
				
				// Price
				new GuiTextCtrl()
				{
					profile = "SkinWindowGoldText";
					canHit = false; 
					text = %price;
					visible = %canBuy && %isPriceVisible;
				};
				
				// Coin icon
				new GuiBitmapCtrl()
				{
					extent = "20 20";
					profile = "GuiAtlas3ImageProfile";
					imageIndex = GoldIcon;
					centered = false;
					canHit = false; 
					wrap = false;
					resize = false;
					visible = %canBuy && %isPriceVisible;
				};
			};
		};
	};
	
	return %guiContent;
}

